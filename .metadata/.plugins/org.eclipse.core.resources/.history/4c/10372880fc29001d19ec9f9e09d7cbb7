import java.awt.*;

class MyFrame8 extends Frame
{
	Button b1,b2,b3,b4,b5,b6;
	
	MyFrame8()
	{
		super("BorderLayoutDemo");
		
		b1=new Button("One");
		b2=new Button("Two");
		b3=new Button("Three");
		b4=new Button("Four");
		b5=new Button("Five");
		b6=new Button("Six");
	
		add(b1);
		add(b2);
		add(b3);
		add(b4);
		add(b5);
		add(b6);
	}
}
public class BorderLayoutDemo {

	public static void main(String[] args) {
		
		MyFrame8 f=new MyFrame8();
		f.setSize(800,800);
		f.setVisible(true);
		
	
}
