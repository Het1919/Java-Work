import java.awt.*;
import java.awt.event.*;

class MyFrame5 extends Frame
{
	TextArea ta;
	TextField tf;
	Label l;
	Button b;
	
	MyFrame5()
	{
		super("TextArea Demo");
		
		l=new Label(" No Text Entered ");
		ta=new TextArea(10);
		tf=new TextField(20);
		b=new Button("Click");
		
		setLayout(new FlowLayout());
		add(ta);
		add(l);
		add(b);
		
	}
}
public class TextAreaDemo {

	public static void main(String[] args) {
		
		MyFrame5 f=new MyFrame5();
		f.setSize(400,400);
		f.setVisible(true);

	}

}
